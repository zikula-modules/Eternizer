{
    "name": "mu/eternizer-module",
    "version": "1.2.0",
    "description": "Eternizer is a small guestbook module.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\EternizerModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "suggest": {
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.0 <3.0",
            "class": "MU\\EternizerModule\\MUEternizerModule",
            "displayname": "Eternizer",
            "url": "eternizer",
            "oldnames": ["Eternizer"],
            "capabilities": {
                "user": {"route": "mueternizermodule_entry_index"},
                "admin": {"route": "mueternizermodule_entry_adminindex"}
            },
            "securityschema": {
                "MUEternizerModule::": "::",
                "MUEternizerModule:ItemListBlock:": "Block title::",
                "MUEternizerModule:ModerationBlock:": "Block title::",
                "MUEternizerModule:Entry:": "Entry ID::",
                "MUEternizerModule::Ajax": "::"
            }
        }
    }
}
